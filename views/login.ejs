<%- include("partials/header.ejs")%>




  <main class="form-signin">
    <form action="<%= body.domain %>/login" method="post">
      <%if (body.message){%>
        <div class="alert alert-success alert-dismissible fade show" role="alert">
          <!-- <strong>Holy guacamole!</strong>  -->
          <%= body.message%>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
        <%}else if(body.error){%>
          <div class="alert alert-danger alert-dismissible fade show" role="alert">
            <strong>Login Failed: </strong>
            <%=body.error%>
              <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
          <%}%>




            <img class="mb-5" style="height: 70px;" src="https://triumphcourier.com/img/Logo-SVG.svg" alt="">

            <script src="https://accounts.google.com/gsi/client" async></script>
            <div class="mb-2 text-center w-100 btn btn-lg btn-block ">

              <div id="g_id_onload"
                data-client_id="81178726386-skurev78vi0ouur9i8ou3dc39j0c7bqi.apps.googleusercontent.com"
                data-login_uri="http://localhost:3060/googleLoggedin" data-auto_prompt="true">
              </div>
              <div class="me-auto g_id_signin" data-type="standard" data-size="large" data-theme="outline"
                data-text="sign_in_with" data-shape="pill" data-width=280 data-logo_alignment="center">
              </div>
            </div>

            <!-- <a href="<%= body.domain %>/auth/google" class="mb-2 w-100 btn btn-lg btn-danger btn-block ">
              <i class="fab fa-google"></i>
              Sign in with Google
            </a> -->

            <p class="mt-5 mb-3 text-muted"><strong>&copy; Dashboard</strong> by ACED Devops 2020 | Triumph Courier
              Services
            </p>

            <!-- <p class="mt-5 mb-0 text-muted">&copy; LS Assistant <%= new Date().getFullYear() %> | built by <a href="https://ejerenwaavis.github.io/cv/"> Avis </a> </p> -->

    </form>
  </main>


